diff --git a/bv9500plus.mk b/bv9500plus.mk
new file mode 100644
index 0000000..6ad15c4
--- /dev/null
+++ b/bv9500plus.mk
@@ -0,0 +1,17 @@
+TARGET_GAPPS_ARCH := arm64
+$(call inherit-product, device/phh/treble/base-pre.mk)
+include build/make/target/product/aosp_arm64.mk
+$(call inherit-product, device/phh/treble/base.mk)
+
+$(call inherit-product, device/phh/treble/gapps.mk)
+$(call inherit-product, device/phh/treble/lineage.mk)
+
+PRODUCT_NAME := bv9500plus
+PRODUCT_DEVICE := bv9500plus
+PRODUCT_BRAND := Blackview
+PRODUCT_SYSTEM_BRAND := Blackview
+PRODUCT_MODEL := BV9500Plus
+PRODUCT_MANUFACTURER := A-gold
+
+PRODUCT_PACKAGES += 
+
diff --git a/bv9500plus/BoardConfig.mk b/bv9500plus/BoardConfig.mk
new file mode 100644
index 0000000..2caf567
--- /dev/null
+++ b/bv9500plus/BoardConfig.mk
@@ -0,0 +1,15 @@
+include build/make/target/board/generic_arm64/BoardConfig.mk
+include device/phh/treble/board-base.mk
+
+ifeq ($(BOARD_SYSTEMIMAGE_PARTITION_RESERVED_SIZE),)
+BOARD_SYSTEMIMAGE_PARTITION_SIZE := 2147483648
+else
+BOARD_SYSTEMIMAGE_PARTITION_RESERVED_SIZE := 33554432
+endif
+
+# Boot Animation
+TARGET_SCREEN_HEIGHT := 1920
+TARGET_SCREEN_WIDTH := 1080
+
+# Charger images
+BOARD_HEALTHD_CUSTOM_CHARGER_RES := device/phh/treble/charger/images
