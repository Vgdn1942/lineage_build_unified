diff --git a/rw-system.sh b/rw-system.sh
index fe760b1..fdb1d08 100644
--- a/rw-system.sh
+++ b/rw-system.sh
@@ -512,6 +512,42 @@ for f in /vendor/lib/mtk-ril.so /vendor/lib64/mtk-ril.so /vendor/lib/libmtk-ril.
     setprop persist.sys.radio.ussd.fix true
 done
 
+if getprop ro.vendor.build.fingerprint | grep -iq -e Blackview/BV9500Plus -e Blackview/BV9500Plus_EEA; then
+    for f in /vendor/ueventd.rc /vendor/bin/hw/android.hardware.light@2.0-service-mediatek \
+            /vendor/etc/init/android.hardware.light@2.0-service-mediatek.rc; do
+        [ ! -f $f ] && continue
+        # shellcheck disable=SC2010
+        ctxt="$(ls -lZ "$f" | grep -oE 'u:object_r:[^:]*:s0')"
+        ctmt="$(stat -c "0%a" "$f")" # mode
+        ctut="$(stat -c "%U" "$f")" # user/owner
+        ctgt="$(stat -c "%G" "$f")" # group
+        a="$(echo "$f" | tr / _)"
+        b="$(basename "$f")" # basename
+        #c="$(echo "$f" | awk -F"/vendor/" '{ print $2 }')" # path
+        #d="$(echo "$c" | rev | cut -d'/' -f2- | rev)" # dir
+        if [ "$b" = "ueventd.rc" ]; then
+            cp -a "$f" "/mnt/phh/$a"
+            echo "" >> "/mnt/phh/$a"
+            for i in 1 2 3 4; do
+                echo "/sys/class/leds/mt6370_pmu_led$i   delay_on      0664  system   system" >> "/mnt/phh/$a"
+                echo "/sys/class/leds/mt6370_pmu_led$i   delay_off      0664  system   system" >> "/mnt/phh/$a"
+            done
+            echo "" >> "/mnt/phh/$a"
+        elif [ "$b" = "android.hardware.light@2.0-service-mediatek" ] || \
+                [ "$b" = "android.hardware.light@2.0-service-mediatek.rc" ]; then
+            [ ! -f /system/phh/$b ] && continue
+            cp -a "/system/phh/$b" "/mnt/phh/$a"
+        else
+            continue
+        fi
+        chmod "$ctmt" "/mnt/phh/$a"
+        chown "$ctut" "/mnt/phh/$a"
+        chgrp "$ctgt" "/mnt/phh/$a"
+        chcon "$ctxt" "/mnt/phh/$a"
+        mount -o bind "/mnt/phh/$a" "$f"
+    done
+fi
+
 if getprop ro.vendor.build.fingerprint | grep -iq -e iaomi/cactus -e iaomi/cereus; then
     setprop debug.stagefright.omx_default_rank.sw-audio 1
     setprop debug.stagefright.omx_default_rank 0
